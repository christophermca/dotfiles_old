[user]
   name = Christopher MCAdams
   email = cmcadams@newrelic.com
[web]
   browser = chrome
[push]
   default = simple
[alias]
   st = status
   co = checkout
   graph = 'git log --oneline --graph --abbrev-commit --pretty=format:"%Cgreen %h %Creset%s %n%an - %cr %n%n"'
   mismatch = ls-files -u
   del = branch -delete --force
   remdel = push origin --no-verify --delete
   updel = push upstream --no-verify --delete
   pullr = pull --rebase
   b = branch
  susy = submodules --sync
  sup = submodules update --init --recursive --remote
[help]
   browser = chrome
[core]
  editory = vim
  fileMode = false
  autocrlf = input
  safecrlf = false
  gitproxy = gitproxy for github
[credential]
	helper = osxkeychain
[diff]
	tool = vimdiff
[difftool]
	prompt = true
[color]
	ui = true
[pull]
	ff = only
